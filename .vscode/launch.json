{
   // Use IntelliSense to learn about possible attributes.
   // Hover to view descriptions of existing attributes.
   // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
   "version": "0.2.0",
   "configurations": [
      {
         "name": "Run dump on helloworld.tb",
         "type": "cppdbg",
         "request": "launch",
         "program": "${workspaceFolder}/build/gasp/gasp",
         "args": [
            "dump",
            "-i",
            "${workspaceFolder}/build/gasp/helloworld.tb"
         ],
         "stopAtEntry": false,
         "cwd": "${workspaceFolder}/build/gasp",
         "environment": [],
         "externalConsole": false,
         "MIMode": "lldb"
      },
      {
         "name": "Run blaise-vscode",
         "type": "extensionHost",
         "request": "launch",
         "runtimeExecutable": "${execPath}",
         "args": [
            "--extensionDevelopmentPath=${workspaceFolder}/blaise-vscode"
         ]
      }
   ]
}